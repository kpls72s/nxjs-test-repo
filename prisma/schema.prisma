generator client {
  provider        = "prisma-client"
  previewFeatures = ["typedSql"]
  output          = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model BlogPost {
  id          String   @id
  title       String
  content     String
  imageUrl    String
  authorID    String
  authorName  String
  authorImage String
  createdAt   DateTime @default(now())
  updatedAt   DateTime
}
